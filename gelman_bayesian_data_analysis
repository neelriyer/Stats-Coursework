      "source": [
        "n = 1000\n",
        "\n",
        "n1 = 10\n",
        "y1 = 6\n",
        "n2 = 20\n",
        "y2 = 10\n",
        "\n",
        "p = 1\n",
        "q = 1\n",
        "\n",
        "p1 = rbeta(n, p + y1, q + n1 - y1)\n",
        "p2 = rbeta(n, p + y2, q + n2 - y2)\n",
        "\n",
        "diff = p1 - p2\n",
        "\n",
        "\n",
        "quantiles = quantile(diff, c(1-alpha/2), names = F)\n",
        "upper_quantile = quantiles[1]\n",
        "upper_quantile\n",
        "\n",
        "mean(p1 > p2)"
      ],
